{
  "name": "beach-forecast",
  "version": "1.0.0",
  "description": "This is the perfect API for surfers! It shows you what the weather is like and evaluates for you which is the best beach to go surfing at the moment.",
  "main": "src/infra/express/index.ts",
  "scripts": {
    "node:dev": "export NODE_ENV=development",
    "start:dev": "yarn node:dev && ts-node-dev -r tsconfig-paths/register -r dotenv/config --inspect --respawn --transpile-only --ignore-watch node_modules --no-notify src/http/express/index.ts | pino-pretty -c",
    "test:integration": "jest --projects ./tests --runInBand",
    "test:unit": "jest --projects .",
    "test": "yarn test:unit && yarn test:integration",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "style": "prettier --write *.ts",
    "prepare": "husky install",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "pm2-runtime start ecosystem.config.js --env production"
  },
  "engines": {
    "node": "16.14.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suares01/Beach-Forecast"
  },
  "keywords": [
    "api",
    "nodejs",
    "typescript",
    "babel",
    "surf",
    "weather",
    "beach",
    "sun"
  ],
  "author": "Lucas Suares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Suares01/Beach-Forecast/issues"
  },
  "homepage": "https://github.com/Suares01/Beach-Forecast.git",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@swc/core": "^1.2.194",
    "@swc/jest": "^0.2.21",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.3",
    "@types/express-rate-limit": "^6.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/mongoose": "^5.11.97",
    "@types/nock": "^11.1.0",
    "@types/node": "^17.0.8",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.11",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "nock": "^13.2.2",
    "pino-pretty": "^7.5.1",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "express-pino-logger": "^7.0.0",
    "express-rate-limit": "^6.2.1",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.0.6",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.1.7",
    "pino": "^7.6.4",
    "pm2": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0",
    "tsyringe": "^4.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
