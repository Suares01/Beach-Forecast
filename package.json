{
  "name": "surf_api",
  "version": "0.0.1",
  "description": "This is the perfect API for surfers! It shows you what the weather is like and evaluates for you which is the best beach to go surfing at the moment.",
  "main": "server.ts",
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register -r dotenv/config --inspect --respawn --transpile-only --ignore-watch node_modules --no-notify src/Server.ts",
    "test:functional": "jest --projects ./tests --runInBand",
    "test:root": "jest --projects .",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "yarn build && node -r dotenv/config dist/Server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Suares01/App_Node_Template"
  },
  "keywords": [
    "api",
    "nodejs",
    "typescript",
    "babel",
    "surf",
    "weather"
  ],
  "author": "Lucas Suares",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Suares01/App_Node_Template/issues"
  },
  "homepage": "https://github.com/Suares01/App_Node_Template.git",
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/config": "^0.0.41",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "jest": "^27.4.7",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "config": "^3.3.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.2"
  }
}
